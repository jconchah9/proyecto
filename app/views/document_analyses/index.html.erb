<p id="notice"><%= notice %></p>

<h1>Document Analyses</h1>

<table class="table">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Tipo</th>
      <th>Descripcion</th>
      <th>Documento</th>
      <th>Analysis</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @document_analyses.each do |document_analysis| %>
      <tr>
        <td><%= document_analysis.name %></td>
        <td><%= document_analysis.type_file %></td>
        <td><%= document_analysis.descripcion %></td>
        <td><%= document_analysis.document %></td>
        <td><%= document_analysis.analysis.id %></td>
        <td><%= link_to 'Show', document_analysis %></td>
        <td><%= link_to 'Edit', edit_document_analysis_path(document_analysis) %></td>
        <td><%= link_to 'Destroy', document_analysis, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Document Analysis', new_document_analysis_path %>
