<p id="notice"><%= notice %></p>
<div class="card black-text">
  <div class="card-content">
    <div class="card-title">
      Presupuestos
    </div>
    <table class="responsive-table centered highlight">
      <thead>
        <tr>
          <th>State</th>
          <th>Summary</th>
          <th colspan="3"></th>
        </tr>
      </thead>
      <tbody>
        <% @budgets.each do |budget| %>
          <tr>
            <td><%= budget.state %></td>
            <td><%= budget.summary %></td>
            <td><%= link_to 'Mostar', budget, class: "green-text text-accent-4" %></td>
            <td><%= link_to 'Editar', edit_budget_path(budget), class: "green-text text-accent-4" %></td>
            <td><%= link_to 'Eliminar', budget, class: "amber-text text-darken-4", method: :delete, data: { confirm: '! Estas seguro? ยก' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
