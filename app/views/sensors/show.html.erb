<div class="container">
  <div class="card">
    <div class="card-action">
      <%= link_to 'Editar', edit_sensor_path(@sensor) %>
      <%= link_to 'Atras', sensors_path %>
    </div>
    <div class="card-content">
      <p>
        <strong>ID sensor:</strong>
        <%= @sensor.id_sensor %>
      </p>
      <p>
        <strong>Tipo sensor:</strong>
        <%= @sensor.type_sensor %>
      </p>
    </div>
  </div>
</div>
